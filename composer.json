{
    "name": "agoalofalife/bpm-online",
    "description": "Using the API BPM Terrasoft",
    "type": "no",
    "license": "MIT",
    "version" : "2.1.2",
    "keywords": [
        "bpm-online",
        "bpm"
    ],
    "require": {
        "php": ">=5.6.4",
        "beberlei/assert": "^2.5",
        "illuminate/container": "~5.4",
        "illuminate/support": "~5.4",
        "illuminate/config": "~5.4",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/var-dumper": "^3.2",
        "monolog/monolog": "1.*",
        "slim/slim": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "5.7.*",
        "php-mock/php-mock-mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "agoalofalife\\bpmOnline\\": "",
            "agoalofalife\\bpm\\" : "src"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "agoalofalife\\Tests\\": "tests/"
        }
    },
        "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure --no-coverage"
    },
    "bin": [
        "panel-server"
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
